{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.0.0",

  "default_locale": "en",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },

  "action": {
    "default_icon": {
      "16": "/icons/icon16.png",
      "32": "/icons/icon32.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  },

  "background": {
    "service_worker": "background.js"
  },

  "commands": {
    "": {
      "suggested_key": {
        "default": ""
      },
      "description": "__MSG_command___"
    },
    "dev": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "dev"
    }
  },

  "permissions": ["contextMenus", "notifications"],

  "chrome": {
    "version_name": "1.0.0",
    "minimum_chrome_version": "92"
  },
  "firefox": {
    "browser_specific_settings": {
      "gecko": {
        "id": "example@example.org",
        "strict_min_version": "42.0"
      }
    },
    "background": {
      "scripts": ["background.js"]
    }
  }
}
